{
  "name": "frida-net",
  "version": "2.0.1",
  "authors": [
    "Frida Developers"
  ],
  "description": "Node.js compatible “net” module for Frida",
  "keywords": [
    "frida"
  ],
  "homepage": "http://www.frida.re",
  "main": "index.js",
  "files": [
    "index.js",
    "lib/adapter.js"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "loose": true
              }
            ]
          ],
          "plugins": [
            [
              "@babel/plugin-transform-runtime",
              {
                "corejs": true
              }
            ]
          ]
        }
      ]
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/frida/frida-net.git"
  },
  "bugs": {
    "url": "http://github.com/frida/frida-net/issues"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "babelify": "^10.0.0",
    "ipaddr.js": "^1.2.0"
  }
}
